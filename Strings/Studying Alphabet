
Question Link - https://www.codechef.com/problems/ALPHABET

1) C++ :

#include <iostream>
using namespace std;

int main() {
	string s;
	cin>>s;
	int t;
	cin>>t;
	while(t--){
	    string st;
	    cin>>st;
	    int cnt=0;
	    for(int i=0;i<s.size();i++){
	        for(int j=0;j<st.size();j++){
	            if(s[i]==st[j]){
	                cnt++;
	            }
	        }
	    }
	    if(cnt==st.size()){
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
