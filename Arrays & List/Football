
Question Link - https://www.codechef.com/problems/MSNSADM1

1) C++ :

#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    int b[n];
	    int ans=INT_MIN;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        cin>>b[i];
	    }
	    for(int i=0;i<n;i++){
	        int x=a[i]*20;
	        int y=b[i]*10;
	        x-=y;
	        if(x<0){x=0;}
	        ans=max(ans,x);
	    }
	   cout<<ans<<endl; 
	}
	return 0;
}
