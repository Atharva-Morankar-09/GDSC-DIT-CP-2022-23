
Question Link - https://www.codechef.com/problems/PAIREQ?tab=statement

1) C++ :

#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int arr[n];
	    for(int i=0;i<n;i++){
	        cin>>arr[i];
	    }
	    sort(arr,arr+n);
	    int ans=0;
	    
	    for(int i=0;i<n-1;i++){
	        int cnt=1;
	        if(arr[i]==arr[i+1]){
	            while(arr[i]==arr[i+1]){
	                cnt++;
	                i++;
	            }
	        }
	        ans=max(ans,cnt);
	    }
	   cout<<n-ans<<endl; 
	}
	return 0;
}

