 Question Link - https://www.codechef.com/CDHS2022/problems/DECPAL
 
 1) C++ : 
 
#include <bits/stdc++.h>
using namespace std;
const int n=10000009;

int main() {
	int t;
	cin>>t;
	while(t--){
	    string a;
	    cin>>a;
	    int i,j,s=0,s2=0,l=a.length(),z=0;
	    int l2=l-1;
	    for(i=0;i<=l2/2;i++){
	        if(a[i]!=a[l-i-1]){
	            if(a[i]=='#' || a[l-1-i]=='#') s++;
	            else{z=1;break;}
	        }
	        else if(a[i]=='#') s2++;
	    }
	    if(z==1) cout<<"No\n";
	    else{
	        long long ans=1;
	        for(i=0;i<s2;i++) ans=((ans%n)*26)%n;
	        if(ans==0){cout<<"No"<<endl;}
	        else{cout<<"Yes"<<endl;}
	   }
	}
	return 0;
}
