1) C++ :

#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    if(n%5==0)
	    {
	        cout<<"False"<<endl;
	    }
	    else
	    {
	        cout<<"True"<<endl;
	    }
	}
	return 0;
}
